{
  "editor.dragAndDrop": false,
  "editor.emptySelectionClipboard": false,
  "editor.fontFamily": "'Fira Code Retina Medium', 'FuraCode NF', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "editor.renderControlCharacters": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120],
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.autoIndent": true,

  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "markdown",
  "files.encoding": "utf8",
  // The default end of line character. Use \n for LF and \r\n for CRLF.
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.bundle": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.bundle/*/**": true
  },

  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.bundle": true
  },

  "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\6\\pwsh.exe",
  "powershell.powerShellExePath": "C:\\Program Files\\PowerShell\\6\\pwsh.exe",
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "newUntitledFile",
  "window.title": "${dirty}${separator}${activeEditorLong}${separator}${appName}",

  // "gitlens.advanced.messages": {
  //   "suppressCommitHasNoPreviousCommitWarning": false,
  //   "suppressCommitNotFoundWarning": false,
  //   "suppressFileNotUnderSourceControlWarning": false,
  //   "suppressGitVersionWarning": false,
  //   "suppressLineUncommittedWarning": false,
  //   "suppressNoRepositoryWarning": false,
  //   "suppressResultsExplorerNotice": false,
  //   "suppressShowKeyBindingsNotice": true
  // },
  // "diffEditor.ignoreTrimWhitespace": false,

  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": 0,
  "window.titleBarStyle": "custom",
  "gitlens.historyExplorer.enabled": true,

  "[powershell]": {
    // The default end of line character. Use \n for LF and \r\n for CRLF.
    "files.eol": "\r\n"
  },
  "breadcrumbs.enabled": true,
  "github.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,

  "puppet.editorService.loglevel": "debug",
  "puppet.editorService.protocol": "tcp",
  "puppet.editorService.tcp.address": "127.0.0.1",
  "puppet.editorService.tcp.port": 8082,
  "workbench.colorTheme": "Cobalt2",
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\6\\pwsh.exe"
}
